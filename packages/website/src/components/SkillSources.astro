  }

  .sources-table tbody tr:hover {
    background: var(--color-bg);
  }

  .highlight-row {
    background: rgba(232, 168, 124, 0.08);
  }

  .highlight-row:hover {
    background: rgba(232, 168, 124, 0.12);
  }

  .project-link {
    display: flex;
    align-items: center;
    gap: var(--spacing-xs);
    text-decoration: none;
    color: var(--color-text);
    transition: color var(--transition-base);
